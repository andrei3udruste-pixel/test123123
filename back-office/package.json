{
  "name": "finalstake-back-office",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate": "openapi-generator-cli generate -i openapi.json -g typescript-angular -o src/app/openapi"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
